<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/bg_dialog_rc_20"
    android:orientation="vertical">

    <TextView
        android:id="@+id/dialog_import_way_csv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/ripple_gray_rc_tl_tr_20"
        android:gravity="center"
        android:paddingVertical="14dp"
        android:text="@string/dialog_import_way_csv"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="18sp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/gray" />

    <TextView
        android:id="@+id/dialog_import_way_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/ripple_gray_rc_bl_br_20"
        android:gravity="center"
        android:paddingVertical="14dp"
        android:text="@string/dialog_import_way_text"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="18sp" />
</LinearLayout>